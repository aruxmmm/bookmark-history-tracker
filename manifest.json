{
  "manifest_version": 3,
  "name": "书签历史追踪器",
  "version": "1.0",
  "description": "追踪和管理Chrome书签的修改历史",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png"
  }
} 